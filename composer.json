{
    "name": "konceiver/laravel-categories",
    "description": "Categories for Eloquent Models",
    "keywords": [
        "konceiver",
        "laravel-categories"
    ],
    "homepage": "https://github.com/konceiver/laravel-categories",
    "license": "MPL-2.0",
    "authors": [
        {
            "name": "Brian Faust",
            "email": "hello@konceiver.dev",
            "homepage": "https://konceiver.dev",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/support": "^8.0",
        "kalnoy/nestedset": "^5.0",
        "spatie/laravel-package-tools": "^1.9",
        "spatie/laravel-sluggable": "^2.3"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "graham-campbell/analyzer": "^3.0",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Konceiver\\Categories\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Konceiver\\Categories\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test:coverage": "vendor/bin/phpunit --coverage-html=.coverage --coverage-clover=clover.xml",
        "format": "vendor/bin/php-cs-fixer fix ./"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Konceiver\\Categories\\Providers\\CategoriesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
